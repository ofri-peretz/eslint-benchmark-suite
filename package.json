{
    "name": "eslint-benchmark-suite",
    "version": "1.0.0",
    "description": "Reproducible benchmarks for ESLint plugins",
    "type": "module",
    "scripts": {
        "benchmark:import": "node scripts/run-benchmark.js import",
        "benchmark:security": "node scripts/run-benchmark.js security",
        "benchmark:ai-security": "node benchmarks/ai-security/run.js",
        "benchmark:all": "node scripts/run-benchmark.js --all",
        "generate:import": "node scripts/generate-fixtures.js import",
        "generate:security": "node scripts/generate-fixtures.js security",
        "generate:all": "node scripts/generate-fixtures.js --all"
    },
    "devDependencies": {
        "eslint": "^9.17.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-import-next": "^2.1.0",
        "eslint-plugin-secure-coding": "latest",
        "eslint-plugin-pg": "latest",
        "eslint-plugin-node-security": "latest",
        "eslint-plugin-jwt": "latest"
    },
    "keywords": [
        "eslint",
        "benchmark",
        "performance"
    ],
    "author": "Ofri Peretz",
    "license": "MIT"
}